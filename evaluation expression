class Solution(object):
    def calculate(self, s):
        if not s:
            return '0'
        stack, num, sign = [], 0, '+'
        for i in range(len(s)):
            if s[i].isdigit():
                num = num*10 + int(s[i])
            if (not s[i].isdigit() and not s[i].isspace()) or i == len(s) - 1:
                if sign == '+':
                    stack.append(num)
                elif sign == "-":
                    stack.append(-num)
                elif sign == "*":
                    stack.append(stack.pop() * num)
                elif sign == "/":
                    temp = stack.pop()
                    ans = temp//num
                    if ans < 0 and temp%num!=0:
                        stack.append(ans + 1)
                    else:
                        stack.append(ans)
                num = 0
                sign = s[i]
        return sum(stack)
    
       
